{
  "name": "crm-insight",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --mode development",
    "dev:clean": "VITE_LOG_LEVEL=none vite --mode development",
    "dev:quiet": "VITE_LOG_LEVEL=warn vite --mode development",
    "dev:prod": "vite --mode production",
    "build": "tsc && vite build",
    "build:dev": "vite build --mode development",
    "build:prod": "vite build --mode production",
    "build:staging": "vite build --mode staging",
    "build:analyze": "tsc && vite build --mode analyze && npx vite-bundle-analyzer dist",
    "build:performance": "tsc && vite build --mode production && npm run analyze:size",
    "analyze:size": "npx bundlesize",
    "analyze:deps": "npx depcheck",
    "preview": "vite preview",
    "preview:dev": "vite preview --mode development",
    "preview:prod": "vite preview --mode production",
    "preview:analyze": "vite preview --mode analyze",
    "test:build": "npm run build && npm run preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "config:validate": "node scripts/validate-environment-config.js",
    "env:setup:dev": "cp env.development.example .env && cp backend/env.development.example backend/.env",
    "env:setup:prod": "cp env.production.example .env && cp backend/env.production.example backend/.env",
    "supabase:status": "node scripts/supabase-cli.js status",
    "supabase:tables": "node scripts/supabase-cli.js tables",
    "supabase:test": "node scripts/test-supabase-integration.js",
    "supabase:cli": "node scripts/supabase-cli.js"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@hello-pangea/dnd": "^18.0.1",
    "@radix-ui/react-accordion": "^1.2.11",
    "@radix-ui/react-alert-dialog": "^1.1.14",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-collapsible": "^1.1.11",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-hover-card": "^1.1.14",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-radio-group": "^1.3.7",
    "@radix-ui/react-scroll-area": "^1.2.9",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slider": "^1.3.5",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.5",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-toast": "^1.2.14",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@radix-ui/react-visually-hidden": "^1.2.3",
    "@supabase/supabase-js": "^2.50.2",
    "@tanstack/react-query": "^5.81.2",
    "@tanstack/react-table": "^8.21.3",
    "@types/redis": "^4.0.10",
    "@types/styled-components": "^5.1.34",
    "axios": "^1.7.9",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "dotenv": "^16.5.0",
    "framer-motion": "^12.18.1",
    "googleapis": "^150.0.1",
    "ioredis": "^5.6.1",
    "lovable-tagger": "^1.1.8",
    "lucide-react": "^0.263.1",
    "react": "^18.3.1",
    "react-colorful": "^5.6.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.58.1",
    "react-router-dom": "^6.28.0",
    "recharts": "^3.0.0",
    "redis": "^5.5.6",
    "styled-components": "^6.1.19",
    "tailwind-merge": "^2.5.4"
  },
  "devDependencies": {
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.15.0",
    "@typescript-eslint/parser": "^7.15.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.4",
    "terser": "^5.43.1",
    "typescript": "^5.2.2",
    "vite": "^5.3.1"
  }
}
